{
  "name": "github-graphql-api-template-action",
  "version": "0.0.0",
  "private": true,
  "description": "Github graphql api request tempalte action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.{ts,graphql,yml}",
    "format-check": "prettier --check **/*.{ts,graphql,yml,json,js}",
    "lint": "eslint src/**/*.ts --fix",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack",
    "codegen": "graphql-codegen -c src/services/github/codegen.yml",
    "main": "ts-node src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonardodalcin/actions-github-graphql-api-template.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Leonardo Vogel Dalcin",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.3",
    "@actions/github": "^2.1.1",
    "graphql": "^15.0.0",
    "graphql-request": "^1.8.2"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "^1.13.2",
    "@graphql-codegen/typescript-graphql-request": "^1.13.2",
    "@graphql-codegen/typescript-operations": "^1.13.2",
    "@graphql-codegen/cli": "^1.21.8",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": false,
    "arrowParens": "avoid"
  },
  "jest": {
    "clearMocks": true,
    "moduleFileExtensions": ["js", "ts"],
    "testEnvironment": "node",
    "testMatch": ["**/*.test.ts"],
    "testRunner": "jest-circus/runner",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "verbose": true
  }
}
